generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PathIndex {
  id        BigInt   @id @default(autoincrement())
  index     Int      @unique
  address   String   @unique
  createdAt DateTime @default(now())
}
