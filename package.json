{
  "name": "bridge.defichain.com",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "1.6.3"
  },
  "engines": {
    "npm": ">=9.1.2",
    "node": ">=v16.18.1"
  },
  "packageManager": "npm@9.1.2"
}
